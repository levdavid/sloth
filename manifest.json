{
  "name": "Sloth",
  "description": "Make the current page sloth",
  "version": "1.1",
  "permissions": [
    "storage",
    "tabs",
    "contextMenus"
  ],
  "background": {
      "scripts": ["bg.js"]
    },
  "content_scripts": [
    {
      "matches": [
      "http://*/*",
      "https://*/*"
      ],
      "js": ["sloth.js"]
    }
  ],
  "manifest_version": 2
}